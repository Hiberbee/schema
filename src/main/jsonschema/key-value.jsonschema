{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://yamlschema.net/",
  "$ref": "key-value.jsonschema",
  "type": "object",
  "propertyNames": {
    "pattern": "^[A-Za-z_][A-Za-z0-9_]*$",
    "anyOf": [
      {
        "type": "string"
      },
      {
        "type": "number"
      },
      {
        "type": "boolean"
      },
      {
        "type": "integer"
      }
    ]
  }
}
